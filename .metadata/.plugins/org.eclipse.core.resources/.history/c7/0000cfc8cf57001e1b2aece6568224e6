	public static int[] removeDuplicates(int[] array) {
		int[] result = array;
		for (int i = 0; i < array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] == array[j]) {
					for (int h = i; h < array.length - 1; h++) {
						int temp = array[h];
						array[h] = array[h + 1];
						array[h + 1] = temp;

					}
					
				}
//				result = new int[array.length - 1];
				System.arraycopy(array, 0, result, 0, array.length - 1);
			}
		}

		return result;

	}

	public static void main(String[] args) {
//		int array[] = { 1, 2, 3 };
//		System.out.println(Arrays.toString(MyArray(array)));

		int array2[] = { 1, 3, 5, 1, 3, 7, 9, 8 };
		System.out.println(Arrays.toString(removeDuplicates(array2)));
	}
}
