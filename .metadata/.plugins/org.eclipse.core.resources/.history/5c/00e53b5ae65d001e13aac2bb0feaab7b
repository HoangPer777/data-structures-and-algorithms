package medium;

public class _7_reverse {
	public static int reverse(int x) {
		int n;
		int result = 0;
		if ((x <= Math.pow(-2, 31)) || (x >= Math.pow(2, 31) - 1)) {
			return 0;
		} else {
			while (x != 0) {
				n = x % 10;
				x /= 10;
				result = result * 10 + n;
			}
			return result;
		}

	}

	public static void main(String[] args) {
		System.out.println(reverse(123));
	}
}
