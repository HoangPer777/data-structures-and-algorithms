package medium;

public class _7_reverse {
	public static int reverse(int x) {
		int result;
		int m = 0;
		long n = 0;
		if ((x <= Math.pow(-2, 31)) || (x >= Math.pow(2, 31) - 1)) {
			return 0;
		} else {
			while (x != 0) {
				result = x % 10;
				x /= 10;
				m = m * 10 + result;
				n = n * 10 + result;
				if (m != n)
					return 0;
			}
			return x;
		}

	}

	public static void main(String[] args) {
		System.out.println(reverse(123));
	}
}
